#include<bits/stdc++.h>
using namespace std;

int main()
{
  long long  int l , r,i;
   cin>>l>>r;
   long long int n =(r-l+1)/2;
   cout<<"YES"<<endl;
    for( i=l;i<=r;i+=2)
    {
        if(n==0)
        {
            break;
        }
       cout<<i<<" "<<i+1<<endl;
       n--;
    }
     return 0;
}
